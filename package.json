{
  "name": "typescript-mongodb",
  "version": "0.0.1",
  "description": "MongoDB API layer with TypeScript support",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/chlouzada/typescript-mongodb.git"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:w": "vitest watch",
    "prepublishOnly": "pnpm run build"
  },
  "license": "MIT",
  "devDependencies": {
    "@egoist/prettier-config": "1.0.0",
    "prettier": "2.8.4",
    "tsup": "6.6.3",
    "typescript": "4.9.5",
    "vite": "^4.3.3",
    "vitest": "0.28.5"
  },
  "dependencies": {
    "mongodb": "^5.2.0",
    "zod": "^3.21.4"
  }
}
